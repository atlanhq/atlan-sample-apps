{
    "applicationId": "events-app",
    "eventRegistration": {
      "consumes": [
        {
          "eventId": "WorkflowTriggeredByUICompleted",
          "eventType": "application_event",
          "eventName": "workflow_end",
          "version": "v1",
          "filters": [
            {
              "path": "event.data.metadata.workflow_type",
              "operator": "==",
              "value": "WorkflowTriggeredByUI"
            },
            {
              "path": "event.data.metadata.workflow_state",
              "operator": "==",
              "value": "completed"
            }
          ]
        }
      ],

      "produces": [
        {
          "eventId": "SampleWorkflowCompleted",
          "eventType": "application_event",
          "eventName": "workflow_end"
        }
      ]
    }
  }