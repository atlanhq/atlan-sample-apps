{
  "id": "Extractor",
  "name": "Extractor",
  "logo": "https://assets.atlan.com/assets/ph-file-cloud-light.svg",
  "config": {
    "properties": {
      "input_file": {
        "type": "string",
        "required": true,
        "ui": {
          "label": "Input JSON file path",
          "placeholder": "e.g., extractor-app-input-table.json",
          "grid": 8
        }
      },
      "output_file": {
        "type": "string",
        "required": true,
        "ui": {
          "label": "Output file path",
          "placeholder": "e.g., transformed_tables.json",
          "grid": 8
        }
      }
    },
    "steps": [
      {
        "id": "payload",
        "title": "Setup",
        "properties": [
          "input_file",
          "output_file"
        ]
      }
    ]
  }
}